<!DOCTYPE html>
<html>
<head>
	<title>OSP Leśniewo</title>
	<link href="/logo.png" rel="icon" type="image/x-icon">
  	<%= auto_discovery_link_tag(:atom, news_path(:atom)) %>
  	<%= stylesheet_link_tag    "application", :media => "all" %>
  	<%= javascript_include_tag "application" %>
  	<%= csrf_meta_tags %>

	<script type="text/javascript">
		$(document).ready(function() {
			$(".fancybox")
			    .attr('rel', 'gallery')
			    .fancybox({
			        beforeShow: function () {
			            /* Add watermark */
			            $('<div class="watermark"></div>')
			                .bind("contextmenu", function (e) {
			                    return false; /* Disables right click */
			                })
			                .prependTo( $.fancybox.inner );   
			        }
			    });
		});
	</script>
</head>
<body>
				<% if notice %>
				<div class="notice">
				<% if notice.match(/Uwaga!/) %>
				<div class="alert alert-error">
				<% elsif notice.match(/Gratulacje!/) %>
				<div class="alert alert-success">
				<% elsif notice.match(/Informacja!/) %>
				<div class="alert alert-info">
				<% end %>
				    <button type="button" class="close" data-dismiss="alert">&times;</button>
				    <strong><%= notice.html_safe %></strong>
				</div>
				</div>
				<% end %>
	<% if current_page?(:controller => 'users_', :action => 'start') %>
	<div class="page page-home">
	<% elsif current_page?(:controller => 'users_', :action => 'about') %>
	<div class="page page-about">
	<% elsif current_page?(:controller => 'users_', :action => 'czlonkowie') %>
	<div class="page page-news">
	<% else %>
	<div class="page page-contact">
	<% end %>
		<div class="header">
			<div>
				<%= image_tag("herb.png", :class => "logo_herb") %>
				<div style="margin-bottom:-37px;margin-top:-13px;">
				<font style="color:white;font-family: 'Exo', sans-serif;font-size:40px;line-height:50px;">OCHOTNICZA STRAŻ POŻARNA <br>
				<font style="color:red">w LEŚNIEWIE</font></font><span>Bogu na chwałę, ludziom na pożytek</span>
				</div>
				<ul>
					<% if current_page?(:controller => 'users_', :action => 'start') %>
					<li class="selected" onclick="location.href = '/'">
					<% else %>
					<li onclick="location.href = '/'">
					<% end %>
						<%= link_to "Start", "/" %>
					</li>
				  	<% if current_page?("/aktualnosci")||current_page?("/aktualnosci_grupuj")||current_page?("/imprezy")||current_page?("/interwencje")||current_page?("/inne")||current_page?("/szkolenia") %>
					<li class="selected" onclick="location.href = '/aktualnosci'">
					<% else %>
					<li onclick="location.href = '/aktualnosci'">
					<% end %>
						<%= link_to "Aktualności", "/aktualnosci" %>
					</li>
					<% if current_page?("/galeria")||current_page?("/galeria_wszystkie") %>
					<li class="selected" onclick="location.href = '/galeria'">
					<% else %>
					<li onclick="location.href = '/galeria'">
					<% end %>
						<%= link_to "Galeria", "/galeria" %>
					</li>
					<% if current_page?(:controller => 'users_', :action => 'czlonkowie') %>
					<li class="selected" onclick="location.href = '/czlonkowie'">
					<% else %>
					<li onclick="location.href = '/czlonkowie'">
					<% end %>
						<%= link_to "Członkowie", "/czlonkowie" %>
					</li>
					<% if current_page?(:controller => 'news', :action => 'otwp') %>
					<li class="selected" onclick="location.href = '/otwp'">
					<% else %>
					<li onclick="location.href = '/otwp'">
					<% end %>
						<%= link_to "OTWP", "/otwp" %>
					</li>
					<% if current_page?(:controller => 'users', :action => 'wynajem_sali') %>
					<li class="selected" onclick="location.href = '/wynajem_sali'">
					<% else %>
					<li onclick="location.href = '/wynajem_sali'">
					<% end %>
						<%= link_to "Wynajem Sali", "/wynajem_sali" %>
					</li>
					<% if current_page?(:controller => 'users_', :action => 'about') %>
					<li class="selected" onclick="location.href = '/about'">
					<% else %>
					<li onclick="location.href = '/about'">
					<% end %>
						<%= link_to "O nas", "/about" %>
					</li>
				</ul>
<% if (current_page?("/"))||(current_page?("/about"))||(current_page?("/czlonkowie"))||(current_page?("/wynajem_sali")) %>
				<div class="featured">
					<h3>TEL. ALARMOWY 998 lub 112</h3>
					<% if current_page?(:controller => 'users_', :action => 'about') %>
					<br><br><br><br>
						<%= link_to "Kontakt >>", "#kontakt", "data-toggle"=>"modal" %>
					<% end %>
				</div>
				<% end %>
			</div>
		</div>
		<div class="body">
			<%= yield %>
			<div class="sidebar">
				<div style="margin-top:-20px;float:right;color:white;"><%= I18n.l Time.now, :format => "%A, %B %d, %Y" %></div>
				<h4>Licznik Zdarzeń</h4>
				<ul>
					<div class="licznik_zdarzen">
					<li>
						<div style="width:100%">
							<h3>Wszystkich Zdarzeń:  <font id="statystyki"><%= @statystyki.pozary+@statystyki.zagrozenia+@statystyki.falarmy %></font></h3>
						</div>
					</li>
					<li>
						<div style="width:100%">
							<h3>Pożary:  <font id="statystyki1"><%= @statystyki.pozary %>
							<% if current_user && ((current_user.username == "Administrator" && current_user.id == 1)||(current_user.username == "strazak"&& current_user.id == 2)) %>
							<%= link_to "[+]","/pozar_plus", :id=>"plus" %>
							<%= link_to "[-]","/pozar_minus", :id=>"plus" %>
							<% end %>
							</font></h3> 
							<h3>Miejscowych Zagrożeń: <font id="statystyki1"><%= @statystyki.zagrozenia %>
							<% if current_user && ((current_user.username == "Administrator" && current_user.id == 1)||(current_user.username == "strazak"&& current_user.id == 2)) %>
							<%= link_to "[+]","/zagrozenia_plus", :id=>"plus" %>
							<%= link_to "[-]","/zagrozenia_minus", :id=>"plus" %>
							<% end %>
							</font></h3> 
							<h3>Alarmy Fałszywe:  <font id="statystyki1"><%= @statystyki.falarmy %>
							<% if current_user && ((current_user.username == "Administrator" && current_user.id == 1)||(current_user.username == "strazak"&& current_user.id == 2)) %>
							<%= link_to "[+]","/alarmy_plus", :id=>"plus" %>
							<%= link_to "[-]","/alarmy_minus", :id=>"plus" %>
							<% end %>
							</font></h3>
							<%= link_to "zobacz wykres »", "/statystyki" %>
						</div>
					</li>
					</div>
				</ul>
				<h4>Kalendarz Zdarzeń</h4>
				  <div id="calendar">
					  <h2 id="month">
    						<%= link_to "<", :month => (@date.beginning_of_month-1).strftime("%Y-%m") %>
    						<%=I18n.l @date , :format => "%B %Y" %>
    						<%= link_to ">", :month => (@date.end_of_month+1).strftime("%Y-%m") %>
  					  </h2>
				<%= calendar_for @news_kalendarz,:first_day_of_week => 1, :year => @date.year, :month => @date.month do |t| %>
				     <%= t.head('Pn', 'Wt', 'Śr', 'Cz', 'Pt', 'So', 'Nd') %>
				  	  <%= t.day(:day_method => :termin) do |date, news| %>
					   <% licznik = 0 %>
					    <% for news in @news_kalendarz %>
						<% if (date.to_s == news.termin.to_s) %>
						 <% licznik += 1 %>
						  <% if licznik == 1 %>
						  <div class="dropdown calendar_pelne">
				    	     	  	<a class="dropdown-toggle" data-toggle="dropdown" href="#"><%= date.day %></a>
						  <% end %>
						<% end %>
					    <% end %>
					    <% if licznik == 0 %>
						<div class="calendar_puste">
						<%= date.day %>
					    <% end %>
						<ul class="dropdown-menu">
        					    <% for news in @news_kalendarz %>
							<% if (date.to_s == news.termin.to_s) %>
          						  <li><%= link_to h(news.tytul), news, :title => news.tytul %></li>
							<% end %>
        					    <% end %>
      						</ul>
					   </div>
				  	  <% end %>
					<% end %>
				  </div>
				<h4>Pogada</h4>
				<div class="pogoda_widget">
				  <div id="tp-widget-default" data-widget="default/polska/pomorskie/lesniewo">
				    <div id="tp-widget-default-beam">
					<a id="tp-widget-default-flink" href="http://www.twojapogoda.pl/polska/pomorskie/lesniewo" target="_blank" title="Leśniewo - prognoza pogody" data-ga="#utm_source=3days&utm_medium=forecast&utm_campaign=widget-free">Pogoda Leśniewo</a><span> z serwisu</span>
				    </div>
				  <script type="text/javascript">
					(function(d, t, i) {
						if (d.getElementById(i)) return;
						var f = d.getElementsByTagName(t)[0], js = d.createElement(t);
						js.id = i; js.src = 'http://www.twojapogoda.pl/widget/main.js';
						f.parentNode.appendChild(js);
					}(document, 'script', 'tp-widget'));
				  </script>
				  </div>
				</div>
			</div>
			<div class="section">
				<div>
					<h4>Galeria</h4>
					<ul>
					<% @zdjecia_stopka.each do |zdjecia| %>
