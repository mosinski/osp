					<li>
						<div style="width:100%">
							<h3>Wszystkich Zdarzeń:  <font id="statystyki"><%= @statystyki.pozary+@statystyki.zagrozenia+@statystyki.falarmy %></font></h3>
						</div>
					</li>
					<li>
						<div style="width:100%">
							<h3>Pożary:  <font id="statystyki1"><%= @statystyki.pozary %>
							<% if current_user && ((current_user.username == "Administrator" && current_user.id == 1)||(current_user.username == "strazak"&& current_user.id == 2)) %>
							<a href="/pozar_plus" id="plus">[+]</a><a href="/pozar_minus" id="plus">[-]</a>
							<% end %>
							</font></h3> 
							<h3>Miejscowych Zagrożeń: <font id="statystyki1"><%= @statystyki.zagrozenia %>
							<% if current_user && ((current_user.username == "Administrator" && current_user.id == 1)||(current_user.username == "strazak"&& current_user.id == 2)) %>
							<a href="/zagrozenia_plus" id="plus">[+]</a><a href="/zagrozenia_minus" id="plus">[-]</a>
							<% end %>
							</font></h3> 
							<h3>Alarmy Fałszywe:  <font id="statystyki1"><%= @statystyki.falarmy %>
							<% if current_user && ((current_user.username == "Administrator" && current_user.id == 1)||(current_user.username == "strazak"&& current_user.id == 2)) %>
							<a href="/alarmy_plus" id="plus">[+]</a><a href="/alarmy_minus" id="plus">[-]</a>
							<% end %>
							</font></h3>
							<a href="/statystyki">zobacz wykres »</a>
						</div>
					</li>
					</div>
				</ul>
				<h4>Przekaż 1%</h4>
				<a href="http://serwer.osp-lesniewo.pl/1proc.png" target="_blank"><%= image_tag("1_proc_Plakat.jpg", "style"=>"width:100%;") %></a>
			</div>
			<div class="section">
				<div>
					<h4>Galeria</h4>
					<ul>
					<% @zdjecia_stopka.each do |zdjecia| %>
						<li>
						<a href="/galeria"><img src="http://serwer.osp-lesniewo.pl/<%= zdjecia.nazwa %>" style="height:115px;width:175px;"></a>
						</li>
					<% end %>
					</ul>
				</div>
				<div class="connect">
					<h4>Linki</h4>
					<a href="http://www.kppsp.puck.pl/" target="_blank"><%= image_tag("psp.png") %></a>
					<a href="http://www.zosprp.pl/" target="_blank"><%= image_tag("zosp.png") %></a>
				</div>
			</div>
		</div>
		<div class="footer">
			<div>
				<ul>
					<li>
						<a href="/">Start</a>
					</li>
					<li>
						<a href="/about">O nas</a>
					</li>
					<li>
						<% if current_user %>
						<%= current_user.username %>
						<%= link_to "Wyloguj", :logout%>
						<% else %>
						<%= link_to "Logowanie", :login %>
						<% end %>
					</li>
					<li>
						<a data-toggle="modal" href="#kontakt">Kontakt</a>
					</li>
					<% if current_user && ((current_user.username == "Administrator" && current_user.id == 1)||(current_user.username == "strazak"&& current_user.id == 2)) %>
					<%= link_to 'Dodaj News', new_news_path %> | 
					<%= link_to 'Lista Zdjęć', images_path %>
					<% else %>
					<iframe src="http://www.facebook.com/plugins/like.php?href=http://osp-lesniewo.pl&layout=standard&show_faces=false&
2.width=250&action=like&colorscheme=dark" scrolling="no" frameborder="0" allowTransparency="true" style="border:none; overflow:hidden; width:70px; height:25px; margin-right:10px;">
3.</iframe><div class="g-plusone" data-href="http://osp-lesniewo.pl/" data-annotation="none"></div>
					<% end %>
				</ul>
				<p>
					Copyright &#169; <%= Time.now.year %>. OSP Leśniewo all rights reserved.
				</p>
			</div>
		</div>
		<div style="margin-top:-50px">
		<center>Projekt <a href="http://m1l05z.pl">Miłosz Osiński</a> i <a href="http://khallmann.pl">Krystian Hallmann</a></center>
		</div>
</div>
