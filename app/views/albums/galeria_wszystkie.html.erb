  <div class="carousel-inner">  
<% @zdjecia.each do |zdjecia| %>
<% if licznik == 0 %>
    <div class="active item">
	<center>
	<img src="http://m1l05z.pl/osp/<%= zdjecia.nazwa %>" style="height:370px;">
	</center>
		<% if (zdjecia.opis != "") && (zdjecia.opis != nil) %>
		<div class="carousel-caption">
                      <p><%= zdjecia.opis %></p>
                </div>
		<% end %>
    </div> 
<% else %>
    <div class="item">
	<center>
	<img src="http://m1l05z.pl/osp/<%= zdjecia.nazwa %>" style="height:370px;">
	</center>
		<% if (zdjecia.opis != "") && (zdjecia.opis != nil) %>
		<div class="carousel-caption">
                      <p><%= zdjecia.opis %></p>
                </div>
		<% end %>
    </div> 
<% end %> 
